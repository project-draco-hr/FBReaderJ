{
  try {
    decode();
    final File file=new File(DIRECTORY_NAME + "/dimage" + myFileNumber);
    final byte[] data=new byte[(int)file.length()];
    final FileInputStream stream=new FileInputStream(file);
    stream.read(data);
    stream.close();
    return data;
  }
 catch (  IOException e) {
    return null;
  }
}
