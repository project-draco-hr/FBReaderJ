{
  if (myCover == NULL_IMAGE) {
    return null;
  }
 else   if (myCover != null) {
    final ZLImage image=myCover.get();
    if (image != null) {
      return image;
    }
  }
  ZLImage image=null;
  try {
    image=getPlugin().readCover(File);
  }
 catch (  BookReadingException e) {
  }
  myCover=image != null ? new WeakReference<ZLImage>(image) : NULL_IMAGE;
  return image;
}
