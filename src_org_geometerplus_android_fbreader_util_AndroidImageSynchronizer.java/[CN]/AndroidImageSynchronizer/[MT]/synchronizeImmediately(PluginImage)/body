{
  final Connection connection=getConnection(image.Plugin);
  return connection.runImmediately(new Runnable(){
    public void run(){
      try {
        image.setRealImage(new ZLBitmapImage(connection.Reader.readBitmap(image.File.getPath(),Integer.MAX_VALUE,Integer.MAX_VALUE)));
      }
 catch (      Throwable t) {
        t.printStackTrace();
      }
    }
  }
);
}
