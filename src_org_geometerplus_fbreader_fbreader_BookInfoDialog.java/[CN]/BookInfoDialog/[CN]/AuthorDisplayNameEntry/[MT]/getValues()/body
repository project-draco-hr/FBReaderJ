{
  if (myValues.size() == 0) {
    final String initial=initialValue();
    boolean addInitial=true;
    for (    Author author : myCollection.authors()) {
      final String name=author.getDisplayName();
      if (addInitial && (name != null && name.equals(initial))) {
        addInitial=false;
      }
      myValues.add(name);
    }
    if (addInitial) {
      myValues.add(initial);
    }
  }
  return myValues;
}
