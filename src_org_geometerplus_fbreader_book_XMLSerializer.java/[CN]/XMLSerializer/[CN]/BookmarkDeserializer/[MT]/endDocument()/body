{
  if (myBookId == -1) {
    return;
  }
  myBookmark=new Bookmark(myId,myBookId,myBookTitle,myText.toString(),myCreationDate,myModificationDate,myAccessDate,myAccessCount,myModelId,myStartParagraphIndex,myStartElementIndex,myStartCharIndex,myEndParagraphIndex,myEndElementIndex,myEndCharIndex,myIsVisible);
}
