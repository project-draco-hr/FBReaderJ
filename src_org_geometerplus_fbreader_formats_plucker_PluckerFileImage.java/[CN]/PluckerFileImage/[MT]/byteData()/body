{
  final InputStream stream=ZLibrary.getInstance().getInputStream(myPath);
  if (stream == null) {
    return new byte[0];
  }
  try {
    stream.skip(myOffset);
    byte[] buffer=new byte[mySize];
    stream.read(buffer,0,mySize);
    return buffer;
  }
 catch (  IOException e) {
  }
  return new byte[0];
}
