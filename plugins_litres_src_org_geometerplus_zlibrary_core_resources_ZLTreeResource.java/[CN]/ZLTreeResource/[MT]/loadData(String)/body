{
  final String fileName=language + ".xml";
  ResourceTreeReader reader=new ResourceTreeReader();
  try {
    reader.readDocument(ourRoot,ZLResourceFile.createResourceFile("resources/zlibrary/" + fileName).getInputStream());
  }
 catch (  IOException e) {
  }
  try {
    reader.readDocument(ourRoot,ZLResourceFile.createResourceFile("resources/application/" + fileName).getInputStream());
  }
 catch (  IOException e) {
  }
}
