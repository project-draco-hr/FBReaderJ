{
switch (event) {
case Added:
case Updated:
{
      final List<Tag> bookTags=book.tags();
      boolean changed=false;
      if (bookTags.isEmpty()) {
        changed&=createTagSubTree(Tag.NULL);
      }
 else       for (      Tag t : bookTags) {
        if (t.Parent == null) {
          changed&=createTagSubTree(t);
        }
      }
      return changed;
    }
case Removed:
  return false;
default :
return false;
}
}
