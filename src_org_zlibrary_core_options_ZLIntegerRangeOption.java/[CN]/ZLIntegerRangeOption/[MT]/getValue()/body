{
  if (!myIsSynchronized) {
    String value=getConfigValue(null);
    if (value != null) {
      try {
        int intValue=Integer.parseInt(value);
        if (intValue < myMinValue) {
          intValue=myMinValue;
        }
 else         if (intValue > myMaxValue) {
          intValue=myMaxValue;
        }
        myValue=intValue;
      }
 catch (      NumberFormatException e) {
      }
    }
    myIsSynchronized=true;
  }
  return myValue;
}
