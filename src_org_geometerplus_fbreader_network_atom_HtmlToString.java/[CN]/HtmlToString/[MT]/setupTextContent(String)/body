{
  if (ATOMConstants.TYPE_HTML.equals(type) || MimeType.TEXT_HTML.Name.equals(type)) {
    myType=Type.Html;
  }
 else   if (ATOMConstants.TYPE_XHTML.equals(type) || MimeType.TEXT_XHTML.Name.equals(type)) {
    myType=Type.XHtml;
  }
 else {
    myType=Type.Text;
  }
  myBuffer.delete(0,myBuffer.length());
}
