{
  myCurrentPosition+=n;
  while (n > 0) {
    if (myBytesReady > n) {
      myBytesReady-=n;
      myPositionInBuffer+=n;
      n=0;
    }
 else {
      n-=myBytesReady;
      myPositionInBuffer=0;
      myBytesReady=myFileInputStream.read(myBuffer);
      if (myBytesReady <= 0) {
        break;
      }
    }
  }
}
