{
  myCurrentPosition+=n;
  if (myBytesReady >= n) {
    myBytesReady-=n;
    myPositionInBuffer+=n;
  }
 else {
    n-=myBytesReady;
    myBytesReady=0;
    n-=myFileInputStream.skip(n);
    while (n > 0) {
      int skipped=myFileInputStream.read(myBuffer,0,n);
      if (skipped <= 0) {
        break;
      }
      n-=skipped;
    }
  }
}
