{
  try {
    decode();
    final File file=new File(myFileName);
    final byte[] data=new byte[(int)file.length()];
    final FileInputStream stream=new FileInputStream(file);
    try {
      stream.read(data);
    }
  finally {
      stream.close();
    }
    return data;
  }
 catch (  IOException e) {
    return null;
  }
}
