{
  if (position < 0) {
    return null;
  }
  if (position < myTree.subTrees().size()) {
    return (NetworkTree)myTree.subTrees().get(position);
  }
  position-=myTree.subTrees().size();
  if (mySpecialItems != null && position < mySpecialItems.size()) {
    return mySpecialItems.get(position);
  }
  return null;
}
