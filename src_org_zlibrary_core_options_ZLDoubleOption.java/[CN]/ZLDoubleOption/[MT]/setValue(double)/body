{
  if (myIsSynchronized && (myValue == value)) {
    return;
  }
  myValue=value;
  myIsSynchronized=true;
  if (myValue == myDefaultValue) {
    unsetConfigValue();
  }
 else   if (Double.isNaN(myValue) && Double.isNaN(myDefaultValue)) {
    unsetConfigValue();
  }
 else {
    setConfigValue("" + myValue);
  }
}
