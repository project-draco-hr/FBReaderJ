{
  if (!myIsSynchronized) {
    String value=myConfig.getValue(myGroup,myOptionName,null);
    if (value != null) {
      try {
        Double doubleValue=Double.parseDouble(value);
        myValue=doubleValue;
      }
 catch (      NumberFormatException e) {
      }
    }
    myIsSynchronized=true;
  }
  return myValue;
}
