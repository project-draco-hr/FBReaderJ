{
  int textStart=start;
  boolean functionFlag=false;
  for (int ptr=start; ptr < end; ++ptr) {
    if (data[ptr] == 0) {
      functionFlag=true;
      if (ptr != textStart) {
        myBufferLength+=ptr - textStart;
      }
    }
 else     if (functionFlag) {
      int paramCounter=(data[ptr]) % 8;
      if (end - ptr > paramCounter + 1) {
        ptr+=paramCounter;
      }
 else {
        ptr=end - 1;
      }
      functionFlag=false;
      textStart=ptr + 1;
    }
  }
  if (end != textStart) {
    myBufferLength+=end - textStart;
  }
}
