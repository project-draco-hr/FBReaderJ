{
  while (myBufferOffset == myBufferLength) {
    if (myRecordIndex + 1 > myHeader.Offsets.length - 1) {
      return false;
    }
    ++myRecordIndex;
    int currentOffset=myHeader.Offsets[myRecordIndex];
    int nextOffset=(myRecordIndex + 1 < myHeader.Offsets.length) ? myHeader.Offsets[myRecordIndex + 1] : 0;
    if (nextOffset < currentOffset) {
      return false;
    }
    try {
      processRecord(nextOffset - currentOffset);
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
  return true;
}
