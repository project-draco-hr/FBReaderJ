{
  if (myModeEntry != null) {
    myModeEntry.setVisible(visible);
    if (visible) {
      ((ScrollingModeEntry)myModeEntry).onMadeVisible();
    }
 else {
      myLinesToKeepEntry.setVisible(false);
      myLinesToScrollEntry.setVisible(false);
      myPercentToScrollEntry.setVisible(false);
    }
  }
}
