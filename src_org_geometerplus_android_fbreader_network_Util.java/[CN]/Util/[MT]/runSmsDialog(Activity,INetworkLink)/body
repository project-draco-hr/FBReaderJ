{
  try {
    final Intent intent=new Intent(SMS_REFILLING_ACTION,Uri.parse(link.getUrlInfo(INetworkLink.URL_MAIN).URL));
    final NetworkAuthenticationManager mgr=link.authenticationManager();
    if (mgr != null) {
      for (      Map.Entry<String,String> entry : mgr.getSmsRefillingData().entrySet()) {
        intent.putExtra(entry.getKey(),entry.getValue());
      }
    }
    if (PackageUtil.canBeStarted(activity,intent,true)) {
      activity.startActivity(intent);
    }
  }
 catch (  ActivityNotFoundException e) {
  }
}
