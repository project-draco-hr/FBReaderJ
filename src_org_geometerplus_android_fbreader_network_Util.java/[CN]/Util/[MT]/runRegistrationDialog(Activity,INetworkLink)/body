{
  try {
    final Intent intent=new Intent(REGISTRATION_ACTION,Uri.parse(link.getUrlInfo(UrlInfo.Type.SignUp).URL));
    if (PackageUtil.canBeStarted(activity,intent,true)) {
      activity.startActivityForResult(new Intent(REGISTRATION_ACTION,Uri.parse(link.getUrlInfo(UrlInfo.Type.SignUp).URL)),USER_REGISTRATION_REQUEST_CODE);
    }
  }
 catch (  ActivityNotFoundException e) {
  }
}
