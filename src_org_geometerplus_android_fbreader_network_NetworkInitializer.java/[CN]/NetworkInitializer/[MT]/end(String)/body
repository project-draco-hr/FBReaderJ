{
  if (myActivity != null) {
    myActivity.runOnUiThread(new Runnable(){
      public void run(){
        if (error == null) {
          if (myActivity != null) {
            myActivity.startService(new Intent(myActivity.getApplicationContext(),LibraryInitializationService.class));
          }
        }
 else {
          showTryAgainDialog(myActivity,error);
        }
      }
    }
);
  }
}
