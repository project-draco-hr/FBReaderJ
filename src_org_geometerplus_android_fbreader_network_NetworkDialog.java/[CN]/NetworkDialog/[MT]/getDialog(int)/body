{
  NetworkDialog dlg=ourInstances.get(Integer.valueOf(id));
  if (dlg == null) {
switch (id) {
case DIALOG_AUTHENTICATION:
      dlg=new AuthenticationDialog();
    break;
case DIALOG_REGISTER_USER:
  dlg=new RegisterUserDialog();
break;
}
if (dlg != null) {
ourInstances.put(Integer.valueOf(id),dlg);
}
}
return dlg;
}
