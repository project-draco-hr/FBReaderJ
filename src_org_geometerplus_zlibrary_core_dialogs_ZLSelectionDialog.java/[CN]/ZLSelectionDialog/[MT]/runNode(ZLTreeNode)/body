{
  if (node == null) {
    return;
  }
  if (node.isFolder()) {
    myHandler.changeFolder(node);
    update();
  }
 else   if (myHandler.isOpenHandler()) {
    if (((ZLTreeOpenHandler)myHandler).accept(node)) {
      exitDialog();
    }
 else {
      update();
    }
  }
 else {
    ((ZLTreeSaveHandler)myHandler).processNode(node);
    update();
  }
}
