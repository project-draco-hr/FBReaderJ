{
  if (url == null) {
    return null;
  }
  if (mimeType == null) {
    mimeType="image/auto";
  }
  if (url.startsWith("http://") || url.startsWith("https://") || url.startsWith("ftp://")) {
    return new NetworkImage(url,mimeType);
  }
 else   if (url.startsWith("data:")) {
  }
  return null;
}
