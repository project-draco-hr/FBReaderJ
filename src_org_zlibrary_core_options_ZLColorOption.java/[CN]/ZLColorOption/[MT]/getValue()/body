{
  if (!myIsSynchronized) {
    String value=myConfig.getValue(myGroup,myOptionName,null);
    if (value != null) {
      try {
        Integer intValue=Integer.parseInt(value);
        myIntValue=intValue;
      }
 catch (      NumberFormatException e) {
      }
    }
    myIsSynchronized=true;
  }
  return new ZLColor(myIntValue);
}
