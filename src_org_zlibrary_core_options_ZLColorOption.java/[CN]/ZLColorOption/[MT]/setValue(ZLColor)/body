{
  if (colorValue != null) {
    int value=colorValue.getIntValue();
    if (myIsSynchronized && (myIntValue == value)) {
      return;
    }
    myIntValue=value;
    myIsSynchronized=true;
    if (myIntValue == myDefaultValue) {
      unsetConfigValue();
    }
 else {
      setConfigValue("" + myIntValue);
    }
  }
}
