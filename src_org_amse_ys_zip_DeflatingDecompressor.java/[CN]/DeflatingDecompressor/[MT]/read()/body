{
  if (myAvailable <= 0) {
    return -1;
  }
  if (myOutBufferLength == 0) {
    fillOutBuffer();
  }
  if (myOutBufferLength == 0) {
    if (myInflatingInProgress) {
      throw new IOException("cannot read from zip");
    }
 else {
      myAvailable=0;
      return -1;
    }
  }
  --myAvailable;
  --myOutBufferLength;
  return myOutBuffer[myOutBufferOffset++];
}
