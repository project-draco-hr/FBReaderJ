{
  runOnUiThread(new Runnable(){
    public void run(){
switch (code) {
default :
        getListAdapter().replaceAll(getCurrentTree().subTrees());
      break;
case StatusChanged:
    setProgressBarIndeterminateVisibility(!myLibrary.isUpToDate());
  break;
case Found:
openSearchResults();
break;
case NotFound:
UIUtil.showErrorMessage(LibraryActivity.this,"bookNotFound");
break;
}
}
}
);
}
