{
  try {
    final ZLPhysicalFile file=book.File.getPhysicalFile();
    if (file == null) {
      return;
    }
    activity.startActivity(new Intent(Intent.ACTION_SEND).setType(FileTypeCollection.Instance.simplifiedMimeType(file).Name).putExtra(Intent.EXTRA_STREAM,Uri.fromFile(file.javaFile())));
  }
 catch (  ActivityNotFoundException e) {
  }
}
