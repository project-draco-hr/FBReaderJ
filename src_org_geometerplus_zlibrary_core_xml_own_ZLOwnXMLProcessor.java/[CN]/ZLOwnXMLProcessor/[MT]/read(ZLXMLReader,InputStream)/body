{
  ZLOwnXMLParser parser=null;
  try {
    parser=new ZLOwnXMLParser(reader,stream,myBufferSize);
    reader.startDocumentHandler();
    parser.doIt();
    reader.endDocumentHandler();
  }
 catch (  IOException e) {
    return false;
  }
 finally {
    if (parser != null) {
      parser.finish();
    }
  }
  return true;
}
