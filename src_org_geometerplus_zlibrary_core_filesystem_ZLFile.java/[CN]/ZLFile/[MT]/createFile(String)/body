{
  if (path == null) {
    return null;
  }
  if (!path.startsWith("/")) {
    return ZLResourceFile.createResourceFile(path);
  }
  int index=path.lastIndexOf(':');
  if (index > 1) {
    return new ZLArchiveEntryFile(createFile(path.substring(0,index)),path.substring(index + 1));
  }
  return new ZLPhysicalFile(path);
}
