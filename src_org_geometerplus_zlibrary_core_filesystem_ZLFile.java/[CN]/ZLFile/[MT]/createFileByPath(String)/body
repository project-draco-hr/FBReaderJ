{
  if (path == null) {
    return null;
  }
  ZLFile cached=ourCachedFiles.get(path);
  if (cached != null) {
    return cached;
  }
  if (!path.startsWith("/")) {
    while (path.startsWith("./")) {
      path=path.substring(2);
    }
    return ZLResourceFile.createResourceFile(path);
  }
  int index=path.lastIndexOf(':');
  if (index > 1) {
    return ZLArchiveEntryFile.createArchiveEntryFile(createFileByPath(path.substring(0,index)),path.substring(index + 1));
  }
  return new ZLPhysicalFile(path);
}
