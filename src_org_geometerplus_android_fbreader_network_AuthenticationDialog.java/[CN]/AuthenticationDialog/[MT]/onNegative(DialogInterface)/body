{
  final NetworkAuthenticationManager mgr=myLink.authenticationManager();
  final Runnable runnable=new Runnable(){
    public void run(){
      if (mgr.isAuthorised(false).Status != ZLBoolean3.B3_FALSE) {
        mgr.logOut();
        sendCancel(false);
      }
    }
  }
;
  UIUtil.wait("signOut",runnable,myActivity);
}
