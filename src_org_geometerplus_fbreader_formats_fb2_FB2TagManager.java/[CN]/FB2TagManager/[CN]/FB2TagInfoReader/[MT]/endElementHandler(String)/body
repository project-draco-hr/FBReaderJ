{
  if (tag == "genre") {
    myCategoryTag=null;
    mySubCategoryTag=null;
    myGenreIds.clear();
  }
 else   if (tag == "subgenre") {
    if (mySubCategoryTag != null) {
      for (      String id : myGenreIds) {
        ArrayList<Tag> list=ourMap.get(id);
        if (list == null) {
          list=new ArrayList<Tag>();
          ourMap.put(id,list);
        }
        list.add(mySubCategoryTag);
      }
    }
    mySubCategoryTag=null;
    myGenreIds.clear();
  }
  return false;
}
