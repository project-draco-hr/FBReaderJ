{
  try {
    AssetFileDescriptor descriptor=myApplication.getAssets().openFd(getPath());
    if (descriptor != null) {
      descriptor.close();
      return true;
    }
  }
 catch (  IOException e) {
  }
  try {
    String[] names=myApplication.getAssets().list(getPath());
    if (names != null && names.length != 0) {
      return true;
    }
  }
 catch (  IOException e) {
  }
  return false;
}
