{
  myReadMetaData=false;
  myReadState=READ_NONE;
  if (!read(fileName)) {
    return false;
  }
  final ArrayList authors=myAuthorList.isEmpty() ? myAuthorList2 : myAuthorList;
  final int len=authors.size();
  for (int i=0; i < len; ++i) {
    myDescription.addAuthor((String)authors.get(i));
  }
  return true;
}
