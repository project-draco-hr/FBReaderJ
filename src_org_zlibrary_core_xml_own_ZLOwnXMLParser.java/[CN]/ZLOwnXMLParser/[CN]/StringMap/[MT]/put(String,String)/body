{
  final int size=mySize++;
  String[] keys=myKeys;
  if (keys.length == size) {
    keys=new String[2 * size];
    System.arraycopy(myKeys,0,keys,0,size);
    myKeys=keys;
    final String[] values=new String[2 * size];
    System.arraycopy(myValues,0,values,0,size);
    myValues=values;
  }
  keys[size]=key;
  myValues[size]=value;
}
