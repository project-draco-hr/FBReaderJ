{
  FB2Tag tag;
  try {
    tag=getTag(qName);
  }
 catch (  IllegalArgumentException e) {
    return;
  }
switch (tag) {
case P:
    endParagraph();
  break;
case SUB:
case SUP:
case CODE:
case EMPHASIS:
case STRONG:
case STRIKETHROUGH:
addControl((byte)tag.ordinal(),false);
break;
case V:
case SUBTITLE:
case TEXT_AUTHOR:
case DATE:
popKind();
endParagraph();
break;
case CITE:
case EPIGRAPH:
popKind();
break;
case POEM:
myInsidePoem=false;
break;
default :
break;
}
}
