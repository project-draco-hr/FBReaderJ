{
  super(context,resource,resourceKey);
  myOption=option;
  setList(new String[]{ON,OFF,UNCHANGED});
switch (option.getValue()) {
case ZLBoolean3.B3_TRUE:
    setInitialValue(ON);
  break;
case ZLBoolean3.B3_FALSE:
setInitialValue(OFF);
break;
case ZLBoolean3.B3_UNDEFINED:
setInitialValue(UNCHANGED);
break;
}
}
