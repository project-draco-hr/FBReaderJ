{
  ZLColor color=myFillColor;
  myPaint.setColor(Color.rgb(color.Red,color.Green,color.Blue));
  if (x1 < x0) {
    int swap=x1;
    x1=x0;
    x0=swap;
  }
  if (y1 < y0) {
    int swap=y1;
    y1=y0;
    y0=swap;
  }
  myCanvas.drawRect(x0,y0,x1 + 1,y1 + 1,myPaint);
  color=myColor;
  myPaint.setColor(Color.rgb(color.Red,color.Green,color.Blue));
}
