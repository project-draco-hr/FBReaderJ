{
  tag=tag.toLowerCase();
  String href=null;
  if ("img".equals(tag)) {
    href=attributes.getValue("src");
  }
 else   if ("image".equals(tag)) {
    href=getAttributeValue(attributes,XLink,"href");
  }
  if (href != null) {
    myImage=new ZLFileImage(MimeType.IMAGE_AUTO,ZLFile.createFileByPath(myXHTMLPathPrefix + MiscUtil.decodeHtmlReference(href)));
    return true;
  }
  return false;
}
