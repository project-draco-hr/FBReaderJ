{
  int leftIndex=0;
  int rightIndex=array.length - 1;
  if (array[rightIndex] <= value) {
    return rightIndex;
  }
  while (leftIndex < rightIndex - 1) {
    int middleIndex=(leftIndex + rightIndex) / 2;
    if (array[middleIndex] <= value) {
      leftIndex=middleIndex;
    }
 else {
      rightIndex=middleIndex;
    }
  }
  return leftIndex;
}
