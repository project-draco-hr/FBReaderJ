{
  FileName=fileName;
  final BooksDatabase database=BooksDatabase.Instance();
  myBookId=database.loadBook(this);
  if (myBookId >= 0) {
    myAuthors=database.loadAuthors(myBookId);
    myTags=database.loadTags(myBookId);
    mySeriesInfo=database.loadSeriesInfo(myBookId);
    myIsSaved=true;
    myIsChanged=false;
  }
}
