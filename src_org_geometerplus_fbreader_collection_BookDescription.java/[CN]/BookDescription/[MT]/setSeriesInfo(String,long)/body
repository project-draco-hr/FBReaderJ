{
  if (mySeriesInfo == null) {
    if (name != null) {
      mySeriesInfo=new SeriesInfo(name,index);
      myIsSaved=false;
    }
  }
 else   if (name == null) {
    mySeriesInfo=null;
    myIsSaved=false;
  }
 else   if (!mySeriesInfo.Name.equals(name) || (mySeriesInfo.Index != index)) {
    mySeriesInfo=new SeriesInfo(name,index);
    myIsSaved=false;
  }
}
