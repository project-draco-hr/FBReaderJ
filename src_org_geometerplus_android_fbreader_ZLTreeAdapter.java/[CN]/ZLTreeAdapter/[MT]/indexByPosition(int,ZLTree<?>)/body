{
  if (position == 0) {
    return 0;
  }
  --position;
  int index=1;
  for (  ZLTree<?> subtree : tree.subTrees()) {
    int count=getCount(subtree);
    if (count <= position) {
      position-=count;
      index+=subtree.getSize();
    }
 else {
      return index + indexByPosition(position,subtree);
    }
  }
  throw new RuntimeException("That's impossible!!!");
}
