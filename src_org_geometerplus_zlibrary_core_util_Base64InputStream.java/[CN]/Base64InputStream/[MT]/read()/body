{
  int result=myDecodedBuffer[0];
  if (result != -1) {
    myDecodedBuffer[0]=-1;
    return result;
  }
  result=myDecodedBuffer[1];
  if (result != -1) {
    myDecodedBuffer[1]=-1;
    return result;
  }
  result=myDecodedBuffer[2];
  if (result != -1) {
    myDecodedBuffer[2]=-1;
    return result;
  }
  fillDecodedBuffer();
  result=myDecodedBuffer[0];
  myDecodedBuffer[0]=-1;
  return result;
}
