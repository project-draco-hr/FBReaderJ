{
  if (super.onFingerReleaseAfterLongPress(x,y)) {
    return true;
  }
  if (myReader.DictionaryTappingActionOption.getValue() == FBReaderApp.DictionaryTappingAction.openDictionary) {
    final ZLTextElementRegion region=currentRegion();
    myReader.doAction(ActionCode.PROCESS_HYPERLINK);
    return true;
  }
  return false;
}
