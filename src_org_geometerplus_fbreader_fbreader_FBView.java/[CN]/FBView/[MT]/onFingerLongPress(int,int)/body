{
  if (super.onFingerLongPress(x,y)) {
    return true;
  }
  if (myReader.DictionaryTappingActionOption.getValue() != FBReaderApp.DictionaryTappingAction.doNothing) {
    final ZLTextElementRegion region=findRegion(x,y,10,ZLTextElementRegion.AnyRegionFilter);
    if (region != null) {
      selectRegion(region);
      myReader.repaintView();
      return true;
    }
  }
  return false;
}
