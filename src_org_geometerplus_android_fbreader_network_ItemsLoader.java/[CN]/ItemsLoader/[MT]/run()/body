{
  try {
    doBefore();
  }
 catch (  ZLNetworkException e) {
    finishOnUiThread(e.getMessage(),false);
    return;
  }
  String error=null;
  try {
    doLoading();
  }
 catch (  ZLNetworkException e) {
    error=e.getMessage();
  }
  ensureItemsProcessed();
  finishOnUiThread(error,isLoadingInterrupted());
  ensureFinishProcessed();
}
