{
  super.startDocumentHandler();
  for (int index=0; ; ++index) {
    final int offset=myMobipocketStream.getImageOffset(index);
    if (offset < 0) {
      break;
    }
    final int length=myMobipocketStream.getImageLength(index);
    if (length <= 0) {
      break;
    }
    addImage(String.valueOf(index + 1),new ZLFileImage(MimeType.IMAGE_AUTO,Model.Book.File,ZLFileImage.ENCODING_NONE,offset,length));
  }
}
