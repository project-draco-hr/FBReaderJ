{
  final ZLTextRegion region=findRegion(x,y,maxDistance,ZLTextRegion.AnyRegionFilter);
  if (region == null) {
    return null;
  }
synchronized (myHighlightings) {
    for (    ZLTextHighlighting h : myHighlightings) {
      if (h.intersects(region)) {
        return h;
      }
    }
  }
  return null;
}
