{
  final ZLFile file=item.getFile();
  final Book book=item.getBook();
  if (book != null) {
    openBook(book);
  }
 else   if (file.isDirectory() || file.isArchive()) {
    startActivity(new Intent(this,FileManager.class).putExtra(SELECTED_BOOK_PATH_KEY,mySelectedBookPath).putExtra(FILE_MANAGER_PATH,file.getPath()));
  }
}
