{
  System.err.println("+++ TOKEN AUTH +++");
  try {
    final String authUrl=params.get("auth-url-token");
    final String clientId=params.get("client-id");
    if (authUrl == null || clientId == null) {
      return false;
    }
    final Intent intent=AccountManager.newChooseAccountIntent(null,null,new String[]{"com.google"},false,null,null,null,null);
    myActivity.startActivityForResult(intent,NetworkLibraryActivity.REQUEST_ACCOUNT_PICKER);
    startWaiting();
    final String authToken=GoogleAuthUtil.getToken(myActivity,myAccount,String.format("audience:server:client_id:%s",clientId));
    ZLNetworkManager.Instance().perform(new ZLNetworkRequest(authUrl,authToken,true){
    }
);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  System.err.println("--- TOKEN AUTH ---");
  return true;
}
