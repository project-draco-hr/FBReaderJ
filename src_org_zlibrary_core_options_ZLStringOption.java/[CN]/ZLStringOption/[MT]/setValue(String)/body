{
  if (myIsSynchronized && (myValue.equals(value))) {
    return;
  }
  if (value != null) {
    myValue=value;
    myIsSynchronized=true;
    if (myValue.equals(myDefaultValue)) {
      myConfig.unsetValue(myGroup,myOptionName);
    }
 else {
      myConfig.setValue(myGroup,myOptionName,myValue,myCategory);
    }
  }
}
