{
  if (value == null) {
    return;
  }
  value=value.intern();
  if (myIsSynchronized && (myValue == value)) {
    return;
  }
  myValue=value;
  if (value == myDefaultValue) {
    myConfig.unsetValue(myGroup,myOptionName);
  }
 else {
    myConfig.setValue(myGroup,myOptionName,value,myCategory);
  }
  myIsSynchronized=true;
}
