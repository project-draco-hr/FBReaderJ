{
  if (getBook() != null) {
    return;
  }
  final TreeSet<ZLFile> set=new TreeSet<ZLFile>(ourFileComparator);
  for (  ZLFile file : myFile.children()) {
    if (file.isDirectory() || file.isArchive() || PluginCollection.Instance().getPlugin(file) != null) {
      set.add(file);
    }
  }
  clear();
  for (  ZLFile file : set) {
    new FileTree(this,file);
  }
}
