{
  if (this == obj) {
    return true;
  }
  if (!(obj instanceof AuthScopeKey)) {
    return false;
  }
  final AuthScope scope=((AuthScopeKey)obj).myScope;
  if (myScope == null) {
    return scope == null;
  }
  if (scope == null) {
    return false;
  }
  return myScope.getPort() == scope.getPort() && MiscUtil.equals(myScope.getHost(),scope.getHost()) && MiscUtil.equals(myScope.getScheme(),scope.getScheme()) && MiscUtil.equals(myScope.getRealm(),scope.getRealm());
}
