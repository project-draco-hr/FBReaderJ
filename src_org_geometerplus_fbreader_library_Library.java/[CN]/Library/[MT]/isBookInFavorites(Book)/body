{
  if (book == null) {
    return false;
  }
  waitForState(STATE_FULLY_INITIALIZED);
  final LibraryTree rootFavorites=getFirstLevelTree(ROOT_FAVORITES);
  for (  FBTree tree : rootFavorites.subTrees()) {
    if (tree instanceof BookTree && book.equals(((BookTree)tree).Book)) {
      return true;
    }
  }
  return false;
}
