{
  System.gc();
  System.gc();
  Book book=getBook(file,fileInfos,savedBooks,doReadMetaInfo);
  if (book != null) {
    myBooks.add(book);
  }
 else   if (file.isArchive()) {
    for (    ZLFile entry : fileInfos.archiveEntries(file)) {
      collectBooks(entry,fileInfos,savedBooks,doReadMetaInfo);
    }
  }
}
