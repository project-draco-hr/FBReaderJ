{
  File[] dirs=myFile.listFiles();
  ArrayList newdirs=new ArrayList();
  if ((dirs == null) && isRoot()) {
    dirs=File.listRoots();
    for (int i=0; i < dirs.length; i++) {
      newdirs.add(dirs[i].getPath());
    }
  }
 else   if (dirs != null) {
    for (int i=0; i < dirs.length; i++) {
      if (dirs[i].isDirectory()) {
        newdirs.add(dirs[i].getName());
      }
    }
  }
  return newdirs;
}
