{
  super.onResume();
  if (myPanel.ControlPanel != null) {
    myPanel.ControlPanel.setVisibility(myPanel.Visible ? View.VISIBLE : View.GONE);
  }
  if (ZLAndroidApplication.Instance().DontTurnScreenOffOption.getValue()) {
    myWakeLock=((PowerManager)getSystemService(POWER_SERVICE)).newWakeLock(PowerManager.SCREEN_DIM_WAKE_LOCK | PowerManager.ON_AFTER_RELEASE,"FBReader");
    myWakeLock.acquire();
  }
 else {
    myWakeLock=null;
  }
}
