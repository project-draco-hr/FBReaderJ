{
  final StringBuilder builder=new StringBuilder();
  boolean flag=false;
  for (  String bookId : Link.basket().bookIds()) {
    if (flag) {
      builder.append(',');
    }
 else {
      flag=true;
    }
    builder.append(bookId);
  }
  return ZLNetworkUtil.appendParameter(getUrl(UrlType.Catalog),"ids",builder.toString());
}
