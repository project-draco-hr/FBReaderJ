{
  if (super.runTreeItem(tree)) {
    return true;
  }
  finish();
  final FBReader fbreader=(FBReader)FBReader.Instance();
  final Book book=((BookTree)tree).Book;
  if ((fbreader.Model == null) || (fbreader.Model.Book.getId() != book.getId())) {
    fbreader.openBook(book,null);
  }
  return true;
}
