{
  if (other == this) {
    return true;
  }
  if (!(other instanceof FontEntry)) {
    return false;
  }
  final FontEntry entry=(FontEntry)other;
  if (!MiscUtil.equals(Family,entry.Family)) {
    return false;
  }
  if (myFileInfos == null) {
    return entry.myFileInfos == null;
  }
  if (entry.myFileInfos == null) {
    return false;
  }
  for (int i=0; i < myFileInfos.length; ++i) {
    if (!MiscUtil.equals(myFileInfos[i],entry.myFileInfos[i])) {
      return false;
    }
  }
  return true;
}
