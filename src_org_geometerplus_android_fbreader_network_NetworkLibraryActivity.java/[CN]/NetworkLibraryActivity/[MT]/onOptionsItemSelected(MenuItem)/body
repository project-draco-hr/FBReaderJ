{
switch (item.getItemId()) {
case MENU_SEARCH:
    return onSearchRequested();
case MENU_ADD_CATALOG:
  AddCustomCatalogItemActions.addCustomCatalog(this);
return true;
case MENU_REFRESH:
refreshCatalogsList();
return true;
case MENU_LANGUAGES:
for (String langCode : NetworkLibrary.Instance().languages()) {
System.err.println("Language: " + ZLLanguageUtil.languageName(langCode));
}
return true;
default :
return true;
}
}
