{
switch (item.getItemId()) {
case MENU_SEARCH:
    return onSearchRequested();
case MENU_ADD_CATALOG:
  AddCustomCatalogItemActions.addCustomCatalog(this);
return true;
case MENU_REFRESH:
refreshCatalogsList();
return true;
case MENU_LANGUAGE_FILTER:
runLanguageFilterDialog();
return true;
default :
return true;
}
}
