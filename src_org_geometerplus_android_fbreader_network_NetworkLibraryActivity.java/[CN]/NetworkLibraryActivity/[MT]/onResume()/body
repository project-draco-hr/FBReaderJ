{
  super.onResume();
  if (!NetworkView.Instance().isInitialized()) {
    if (ourInitializator == null) {
      ourInitializator=new Initializator(this);
      ourInitializator.start();
    }
 else {
      ourInitializator.setActivity(this);
    }
  }
 else {
    prepareView();
    if (myIntent != null) {
      processIntent(myIntent);
      myIntent=null;
    }
  }
}
