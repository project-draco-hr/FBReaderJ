{
  super.onResume();
  if (!NetworkView.Instance().isInitialized()) {
    if (myInitializator == null) {
      myInitializator=new Initializator(this);
      myInitializator.start();
    }
 else {
      myInitializator.setActivity(this);
    }
  }
 else {
    prepareView();
    if (myIntent != null) {
      processIntent(myIntent);
      myIntent=null;
    }
  }
}
