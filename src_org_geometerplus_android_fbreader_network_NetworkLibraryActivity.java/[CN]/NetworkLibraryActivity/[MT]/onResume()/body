{
  super.onResume();
  if (!NetworkView.Instance().isInitialized()) {
    if (NetworkInitializer.Instance == null) {
      new NetworkInitializer(this);
      NetworkInitializer.Instance.start();
    }
 else {
      NetworkInitializer.Instance.setActivity(this);
    }
  }
 else {
    prepareView();
    if (myIntent != null) {
      processIntent(myIntent);
      myIntent=null;
    }
  }
}
