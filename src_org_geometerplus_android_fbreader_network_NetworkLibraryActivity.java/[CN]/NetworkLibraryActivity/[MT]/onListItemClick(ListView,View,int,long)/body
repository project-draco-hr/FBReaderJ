{
  if (myListClickActions.isEmpty()) {
    fillListClickList();
  }
  final NetworkTree tree=(NetworkTree)getListAdapter().getItem(position);
  for (  Action a : myListClickActions) {
    if (a.isVisible(tree) && a.isEnabled(tree)) {
      a.checkAndRun(tree);
      return;
    }
  }
  listView.showContextMenuForChild(view);
}
