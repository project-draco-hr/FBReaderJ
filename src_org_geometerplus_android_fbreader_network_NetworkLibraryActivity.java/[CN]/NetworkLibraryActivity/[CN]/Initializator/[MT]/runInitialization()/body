{
  ((ZLAndroidDialogManager)ZLAndroidDialogManager.Instance()).wait("loadingNetworkLibrary",new Runnable(){
    public void run(){
      String error=null;
      try {
        NetworkView.Instance().initialize();
      }
 catch (      ZLNetworkException e) {
        error=e.getMessage();
      }
      Initializator.this.end(error);
    }
  }
,myActivity);
}
