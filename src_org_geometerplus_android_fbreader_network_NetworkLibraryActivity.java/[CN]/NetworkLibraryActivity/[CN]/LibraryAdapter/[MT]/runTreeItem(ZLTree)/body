{
  if (super.runTreeItem(tree)) {
    return true;
  }
  if (tree instanceof NetworkCatalogTree) {
    NetworkCatalogTree catalogTree=(NetworkCatalogTree)tree;
    if (!catalogTree.hasChildren()) {
      updateCatalogChildren(catalogTree);
      resetTree();
    }
    super.runTreeItem(tree);
    return true;
  }
  return false;
}
