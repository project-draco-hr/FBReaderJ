{
  super.onResume();
  ControlButtonPanel.restoreVisibilities();
  if (ZLAndroidApplication.Instance().DontTurnScreenOffOption.getValue()) {
    myWakeLock=((PowerManager)getSystemService(POWER_SERVICE)).newWakeLock(PowerManager.SCREEN_BRIGHT_WAKE_LOCK,"FBReader");
    myWakeLock.acquire();
  }
 else {
    myWakeLock=null;
  }
}
