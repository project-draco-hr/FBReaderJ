{
  if (file == null) {
    return null;
  }
  Book book=myFBReaderApp.Collection.getBookByFile(file);
  if (book != null) {
    return book;
  }
  if (file.isArchive()) {
    for (    ZLFile child : file.children()) {
      book=myFBReaderApp.Collection.getBookByFile(child);
      if (book != null) {
        return book;
      }
    }
  }
  return null;
}
