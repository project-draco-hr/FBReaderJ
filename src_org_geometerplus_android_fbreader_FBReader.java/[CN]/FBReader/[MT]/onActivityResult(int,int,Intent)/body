{
switch (requestCode) {
case REQUEST_PREFERENCES:
case REQUEST_BOOK_INFO:
    onPreferencesUpdate(resultCode,SerializerUtil.deserializeBook(data.getStringExtra(BookInfoActivity.CURRENT_BOOK_KEY)));
  break;
case REQUEST_CANCEL_MENU:
((FBReaderApp)FBReaderApp.Instance()).runCancelAction(resultCode - 1);
break;
}
}
