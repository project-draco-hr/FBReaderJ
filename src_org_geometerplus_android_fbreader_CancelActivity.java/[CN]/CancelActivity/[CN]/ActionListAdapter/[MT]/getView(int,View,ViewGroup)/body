{
  final View view=convertView != null ? convertView : LayoutInflater.from(parent.getContext()).inflate(R.layout.cancel_item,parent,false);
  ((TextView)view.findViewById(R.id.cancel_item_title)).setText(myIntent.getStringExtra(ITEM_TITLE + position));
  ((TextView)view.findViewById(R.id.cancel_item_summary)).setText(myIntent.getStringExtra(ITEM_SUMMARY + position));
  return view;
}
