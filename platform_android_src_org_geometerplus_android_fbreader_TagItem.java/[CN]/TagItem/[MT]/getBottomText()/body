{
  StringBuilder builder=new StringBuilder();
  int count=0;
  for (  BookDescription description : BookCollection.Instance().booksByTag(myTag)) {
    if (count++ > 0) {
      builder.append(",  ");
    }
    builder.append(description.getTitle());
    if (count == 5) {
      break;
    }
  }
  return builder.toString();
}
