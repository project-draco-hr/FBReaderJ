{
  localName=localName.toLowerCase();
switch (myDepth) {
case 0:
    if (!localName.equals("config")) {
      printError(localName);
    }
  break;
case 1:
if (localName.equals("group")) {
  myCurrentGroup=atts.getValue("name");
}
 else {
  printError(localName);
}
break;
case 2:
if (localName.equals("option")) {
myConfig.setValue(myCurrentGroup,atts.getValue("name"),atts.getValue("value"),myCategory);
}
 else {
printError(localName);
}
break;
default :
System.out.println("too many nesting elements!");
}
myDepth++;
}
