{
  if (Util.BROWSER_ACTION_ID.equals(actionId)) {
    return new Runnable(){
      public void run(){
        Util.openInBrowser(activity,link.authenticationManager().topupLink());
      }
    }
;
  }
 else {
    return new Runnable(){
      public void run(){
        Util.runTopupDialog(activity,link,actionId);
      }
    }
;
  }
}
