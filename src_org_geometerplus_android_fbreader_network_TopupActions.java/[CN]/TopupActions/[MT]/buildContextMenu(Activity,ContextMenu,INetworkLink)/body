{
  menu.setHeaderTitle(getTitleValue("topupTitle"));
  int i=0;
  final List<PluginApi.TopupActionInfo> infos=NetworkView.Instance().TopupActionInfos.get(link.getUrlInfo(UrlInfo.Type.Catalog).Url);
  if (infos != null) {
    for (    PluginApi.TopupActionInfo info : infos) {
      addMenuItemWithText(menu,i++,info.MenuItemName);
    }
  }
}
