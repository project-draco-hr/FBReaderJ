{
  try {
    final List<PluginApi.TopupActionInfo> infos=NetworkView.Instance().TopupActionInfos.get(link.getUrlInfo(UrlInfo.Type.Catalog).Url);
    final String url=infos.get(actionCode).getId().toString();
    doTopup(activity,link,new Runnable(){
      public void run(){
        Util.runTopupDialog(activity,link,url);
      }
    }
);
    return true;
  }
 catch (  Exception e) {
    return false;
  }
}
