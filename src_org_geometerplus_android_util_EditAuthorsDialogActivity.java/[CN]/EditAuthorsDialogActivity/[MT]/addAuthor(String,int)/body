{
  if (author.length() != 0 && author.matches("[\\p{L}0-9_\\- ]*")) {
    if (position < 0) {
      if (!myEditList.contains(author)) {
        myEditList.add(author);
      }
    }
 else {
      myEditList.set(position,author);
    }
    ((BaseAdapter)getListAdapter()).notifyDataSetChanged();
  }
}
