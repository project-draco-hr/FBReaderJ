{
  for (  FBTree tree : getRootTree().subTrees()) {
    if (tree instanceof NetworkCatalogRootTree) {
      final String cUrl=((NetworkCatalogTree)tree).getLink().getUrlInfo(UrlInfo.Type.Catalog).Url;
      if (url.equals(cUrl)) {
        return (NetworkTree)tree;
      }
    }
  }
  return null;
}
