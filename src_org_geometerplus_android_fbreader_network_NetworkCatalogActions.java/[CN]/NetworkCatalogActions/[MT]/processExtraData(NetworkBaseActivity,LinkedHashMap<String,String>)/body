{
  if (extraData != null && !extraData.isEmpty()) {
    final String pluginPackage=extraData.get("androidPlugin");
    if (pluginPackage != null) {
      final ZLResource dialogResource=ZLResource.resource("dialog");
      final ZLResource buttonResource=dialogResource.getResource("button");
      if (!PluginUtil.isPluginInstalled(activity,pluginPackage)) {
        final String text=extraData.get("androidPluginInstallMessage");
        new AlertDialog.Builder(activity).setTitle(dialogResource.getResource("installPlugin").getResource("title").getValue()).setMessage(text).setIcon(0).setPositiveButton(buttonResource.getResource("install").getValue(),null).setNegativeButton(buttonResource.getResource("skip").getValue(),null).create().show();
      }
    }
  }
}
