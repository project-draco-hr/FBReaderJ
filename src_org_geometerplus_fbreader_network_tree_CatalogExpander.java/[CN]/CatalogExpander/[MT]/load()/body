{
  if (myResumeNotLoad) {
    getTree().Item.resumeLoading(this);
  }
 else {
    try {
      getTree().Item.loadChildren(this);
    }
 catch (    NetworkCatalogItem.AuthorisationFailed f) {
      if (myAuthenticator != null) {
        myAuthenticator.run(f.URL);
      }
      System.err.println("AUTH URI XX = " + f.URL);
    }
  }
}
