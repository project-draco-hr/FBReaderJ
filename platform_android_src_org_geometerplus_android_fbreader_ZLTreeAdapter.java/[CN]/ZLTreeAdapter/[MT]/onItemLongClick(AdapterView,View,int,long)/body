{
  if (myContextMenuInProgress) {
    return false;
  }
  System.err.println("onItemLongClick");
  final ZLTree tree=getItem(position);
  if (tree.hasChildren()) {
    myContextMenuInProgress=true;
    view.showContextMenu();
    myContextMenuInProgress=false;
    return true;
  }
  return false;
}
