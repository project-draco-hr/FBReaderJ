{
  if (Model != null) {
    if (book == null || bookmark == null && book.File.equals(Model.Book.File)) {
      return;
    }
  }
  final SynchronousExecutor executor=createExecutor("loadingBook");
  executor.execute(new Runnable(){
    public void run(){
      openBookInternal(book,bookmark,false);
    }
  }
,postAction);
}
