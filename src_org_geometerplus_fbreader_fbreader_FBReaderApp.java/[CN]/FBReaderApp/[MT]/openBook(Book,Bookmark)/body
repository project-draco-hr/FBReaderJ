{
  if (book == null) {
    return;
  }
  if (Model != null) {
    if (bookmark == null & book.File.getPath().equals(Model.Book.File.getPath())) {
      return;
    }
  }
  ZLMainWindowUtil.Instance().wait("loadingBook",new Runnable(){
    public void run(){
      openBookInternal(book,bookmark);
    }
  }
);
}
