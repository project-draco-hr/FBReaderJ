{
  if (index < 0 || index >= myCancelActionsList.size()) {
    return;
  }
  final CancelActionDescription description=myCancelActionsList.get(index);
switch (description.Type) {
case previousBook:
    openBook(Library.getPreviousBook(),null);
  break;
case gotoPosition:
break;
case close:
closeWindow();
break;
}
}
