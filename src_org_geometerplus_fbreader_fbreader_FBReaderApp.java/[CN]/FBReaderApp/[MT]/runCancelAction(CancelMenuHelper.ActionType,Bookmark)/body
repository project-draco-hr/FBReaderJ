{
  if (type == null) {
    return;
  }
switch (type) {
case library:
    runAction(ActionCode.SHOW_LIBRARY);
  break;
case networkLibrary:
runAction(ActionCode.SHOW_NETWORK_LIBRARY);
break;
case previousBook:
openBook(Collection.getRecentBook(1),null,null);
break;
case returnTo:
if (bookmark != null) {
Collection.deleteBookmark(bookmark);
gotoBookmark(bookmark);
}
break;
case close:
closeWindow();
break;
}
}
