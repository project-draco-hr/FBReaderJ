{
switch (type) {
case library:
    runAction(ActionCode.SHOW_LIBRARY);
  break;
case networkLibrary:
runAction(ActionCode.SHOW_NETWORK_LIBRARY);
break;
case previousBook:
openBook(Collection.getRecentBook(1),null,null);
break;
case returnTo:
Collection.deleteBookmark(bookmark);
gotoBookmark(bookmark,true);
break;
case close:
closeWindow();
break;
}
}
