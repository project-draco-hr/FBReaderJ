{
  String value=myCache.get(id);
  if (value == null) {
    try {
      value=getValueInternal(id.Group,id.Name);
    }
 catch (    NotAvailableException e) {
      return defaultValue;
    }
    if (value == null) {
      value=myNullString;
    }
    myCache.put(id,value);
  }
  return value != myNullString ? value : defaultValue;
}
