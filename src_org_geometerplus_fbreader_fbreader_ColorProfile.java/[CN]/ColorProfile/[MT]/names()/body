{
  if (ourNames.isEmpty()) {
    final int size=new ZLIntegerOption("Colors","NumberOfSchemes",0).getValue();
    if (size == 0) {
      ourNames.add(DAY);
      ourNames.add(NIGHT);
    }
 else     for (int i=0; i < size; ++i) {
      ourNames.add(new ZLStringOption("Colors","Scheme" + i,"").getValue());
    }
  }
  return Collections.unmodifiableList(ourNames);
}
