{
  final Locale locale=Locale.getDefault();
  ZLResourceFile file=ZLResourceFile.createResourceFile("data/help/MiniHelp." + locale.getLanguage() + "_"+ locale.getCountry()+ ".fb2");
  if (file.exists()) {
    return file;
  }
  file=ZLResourceFile.createResourceFile("data/help/MiniHelp." + locale.getLanguage() + ".fb2");
  if (file.exists()) {
    return file;
  }
  return ZLResourceFile.createResourceFile("data/help/MiniHelp.en.fb2");
}
