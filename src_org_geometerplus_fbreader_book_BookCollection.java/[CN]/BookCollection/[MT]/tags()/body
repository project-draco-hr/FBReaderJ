{
  final Set<Tag> tags=new HashSet<Tag>();
synchronized (myBooksByFile) {
    for (    Book book : myBooksByFile.values()) {
      final List<Tag> bookTags=book.tags();
      if (bookTags.isEmpty()) {
        tags.add(Tag.NULL);
      }
 else {
        for (        Tag t : bookTags) {
          for (; t != null; t=t.Parent) {
            tags.add(t);
          }
        }
      }
    }
  }
  return new ArrayList<Tag>(tags);
}
