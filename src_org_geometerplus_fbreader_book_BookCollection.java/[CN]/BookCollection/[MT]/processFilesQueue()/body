{
synchronized (myFilesToRescan) {
    if (myBuildStatus != BuildStatus.Finished) {
      return;
    }
    for (    ZLFile file : collectPhysicalFiles(myFilesToRescan)) {
      final Book book=getBookByFile(file);
      if (book != null) {
        saveBook(book,false);
        addBook(book,false);
      }
    }
    myFilesToRescan.clear();
  }
}
