{
  if (prefix == null) {
    return Collections.emptyList();
  }
  final LinkedList<Book> filtered=new LinkedList<Book>();
  for (  Book b : books(new Query(new Filter.Empty(),1000,0))) {
    if (b != null && prefix.equals(b.firstTitleLetter())) {
      filtered.add(b);
    }
  }
  return filtered;
}
