{
  final NetworkLibrary library=NetworkLibrary.Instance();
  final ArrayList<String> ids=new ArrayList<String>(library.activeIds());
  final ArrayList<String> inactiveIds=new ArrayList<String>(library.linkIds());
  inactiveIds.removeAll(ids);
  OrientationUtil.startActivity(myActivity,new Intent(myActivity.getApplicationContext(),AllCatalogsActivity.class).putStringArrayListExtra(AllCatalogsActivity.IDS_LIST,ids).putStringArrayListExtra(AllCatalogsActivity.INACTIVE_IDS_LIST,inactiveIds));
}
