{
  final NetworkItemsLoader loader=NetworkLibrary.Instance().getStoredLoader(tree);
  if (loader != null && loader.canResumeLoading()) {
    Util.openTree(activity,tree);
    return;
  }
  if (loader == null) {
    expandRunnable.run();
  }
 else {
    loader.setPostRunnable(expandRunnable);
  }
}
