{
  final NetworkItemsLoader loader=NetworkLibrary.Instance().getStoredLoader(tree);
  if (loader != null && loader.canResumeLoading()) {
    doOpenTree(tree);
  }
 else   if (loader != null) {
    loader.setPostRunnable(new Runnable(){
      public void run(){
        doLoadCatalog(tree);
      }
    }
);
  }
 else {
    doLoadCatalog(tree);
  }
}
