{
  boolean resumeNotLoad=false;
  if (tree.hasChildren()) {
    if (tree.isContentValid()) {
      if (tree.Item.supportsResumeLoading()) {
        resumeNotLoad=true;
      }
 else {
        doOpenTree(tree);
        return;
      }
    }
 else {
      tree.clearCatalog();
    }
  }
  tree.startItemsLoader(true,resumeNotLoad);
  processExtraData(tree.Item.extraData(),new Runnable(){
    public void run(){
      doOpenTree(tree);
    }
  }
);
}
