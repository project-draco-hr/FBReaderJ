{
  List<String> ids=bookIds();
  if (!ids.contains(book.Id)) {
    ids=new ArrayList<String>(ids);
    ids.add(book.Id);
    myBooksInBasketOption.setValue(ids);
    if (myTree != null) {
      myTree.addItem(book);
    }
 else {
      addItem(book);
    }
    NetworkLibrary.Instance().fireModelChangedEvent(NetworkLibrary.ChangeListener.Code.SomeCode);
  }
}
