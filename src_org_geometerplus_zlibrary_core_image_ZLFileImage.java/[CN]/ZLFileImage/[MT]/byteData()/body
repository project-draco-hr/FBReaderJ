{
  try {
    final InputStream stream=myFile.getInputStream();
    int toSkip=myOffset - (int)stream.skip(myOffset);
    while (--toSkip >= 0) {
      stream.read();
    }
    byte[] buffer=new byte[myLength];
    stream.read(buffer);
    stream.close();
    return buffer;
  }
 catch (  IOException e) {
  }
  return new byte[0];
}
