{
  InputStream stream=null;
  try {
    stream=myFile.getInputStream();
    int toSkip=myOffset - (int)stream.skip(myOffset);
    while (--toSkip >= 0) {
      stream.read();
    }
    byte[] buffer=new byte[myLength];
    int len=0;
    while (len < myLength) {
      final int part=stream.read(buffer);
      if (part <= 0) {
        return new byte[0];
      }
      len+=part;
    }
    return buffer;
  }
 catch (  IOException e) {
    return new byte[0];
  }
 finally {
    if (stream != null) {
      try {
        stream.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
