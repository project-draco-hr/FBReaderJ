{
  final int size=mySize++;
  String[] keys=myKeys;
  if (keys.length == size) {
    String[] tmp=new String[2 * size];
    System.arraycopy(keys,0,tmp,0,size);
    keys=tmp;
    myKeys=keys;
    tmp=new String[2 * size];
    System.arraycopy(myValues,0,tmp,0,size);
    myValues=tmp;
  }
  keys[size]=key;
  myValues[size]=value;
}
