  return supportsAllOrientations() ? new String[]{SCREEN_ORIENTATION_SYSTEM,SCREEN_ORIENTATION_PORTRAIT,SCREEN_ORIENTATION_LANDSCAPE,SCREEN_ORIENTATION_REVERSE_PORTRAIT,SCREEN_ORIENTATION_REVERSE_LANDSCAPE} : new String[]{SCREEN_ORIENTATION_SYSTEM,SCREEN_ORIENTATION_PORTRAIT,SCREEN_ORIENTATION_LANDSCAPE};
