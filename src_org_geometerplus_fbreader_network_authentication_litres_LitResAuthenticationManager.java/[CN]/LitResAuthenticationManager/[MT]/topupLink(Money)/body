{
  final String sid;
synchronized (this) {
    sid=mySidOption.getValue();
  }
  if (sid.length() == 0) {
    return null;
  }
  String url=Link.getUrl(UrlInfo.Type.TopUp);
  if (url == null) {
    return null;
  }
  url=ZLNetworkUtil.appendParameter(url,"sid",sid);
  url=ZLNetworkUtil.appendParameter(url,"summ",String.valueOf(sum.Amount));
  return url;
}
