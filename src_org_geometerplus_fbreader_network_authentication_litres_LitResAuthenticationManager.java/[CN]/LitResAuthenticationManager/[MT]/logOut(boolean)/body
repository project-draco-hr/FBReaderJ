{
  if ("".equals(mySidOption.getValue())) {
    return;
  }
  if (full) {
    initUser(null,"","",false);
  }
 else {
    myFullyInitialized=false;
  }
  myInitializedDataSid=null;
  myPurchasedBooks.clear();
  NetworkLibrary.Instance().fireModelChangedEvent(NetworkLibrary.ChangeListener.Code.SignedIn);
}
