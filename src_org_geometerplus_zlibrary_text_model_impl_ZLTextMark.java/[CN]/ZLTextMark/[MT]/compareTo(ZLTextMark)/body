{
  if (ParagraphNumber == mark.ParagraphNumber) {
    if (Offset == mark.Offset) {
      return 0;
    }
 else     if (Offset < mark.Offset) {
      return -1;
    }
 else {
      return 1;
    }
  }
 else   if (ParagraphNumber < mark.ParagraphNumber) {
    return -1;
  }
 else {
    return 1;
  }
}
