{
  if (myChildrenString == null) {
    StringBuilder builder=new StringBuilder();
    int count=0;
    for (    LibraryTree subtree : subTrees()) {
      if (count++ > 0) {
        builder.append(",  ");
      }
      builder.append(subtree.getName());
      if (count == 5) {
        break;
      }
    }
    myChildrenString=builder.toString();
  }
  return myChildrenString;
}
