{
switch (event) {
default :
case Added:
    return false;
case Removed:
  return removeBook(book,true);
case Updated:
{
  boolean changed=false;
  for (  FBTree tree : this) {
    if (tree instanceof BookTree) {
      final Book b=((BookTree)tree).Book;
      if (b.equals(book)) {
        b.updateFrom(book);
        changed=true;
      }
    }
  }
  return changed;
}
}
}
