{
  final FormatPlugin plugin=PluginCollection.Instance().getPlugin(book.File);
  if (plugin == null) {
    return null;
  }
  final BookModel model;
switch (plugin.type()) {
case NATIVE:
    model=new NativeBookModel(book);
  break;
case JAVA:
model=new JavaBookModel(book);
break;
default :
return null;
}
if (plugin.readModel(model)) {
return model;
}
return null;
}
