{
  while (networkRequest != null) {
    try {
      ZLNetworkManager.Instance().perform(networkRequest);
    }
 catch (    ZLNetworkException e) {
      myLoadingState=null;
      throw e;
    }
    if (listener.confirmInterrupt()) {
      return;
    }
    networkRequest=myLoadingState.resume();
  }
}
