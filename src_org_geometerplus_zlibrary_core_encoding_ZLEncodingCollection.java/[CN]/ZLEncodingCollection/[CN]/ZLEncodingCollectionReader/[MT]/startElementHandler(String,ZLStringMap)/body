{
  if (ENCODING == tag) {
    myCurrentEncodingName=attributes.getValue(NAME);
  }
 else   if (myCurrentEncodingName != null) {
    String alias=null;
    if (ALIAS == tag) {
      alias=attributes.getValue(NAME);
    }
 else     if (CODE == tag) {
      alias=attributes.getValue(NUMBER);
    }
    if (alias != null) {
      myEncodingByAlias.put(alias,myCurrentEncodingName);
    }
  }
  return false;
}
