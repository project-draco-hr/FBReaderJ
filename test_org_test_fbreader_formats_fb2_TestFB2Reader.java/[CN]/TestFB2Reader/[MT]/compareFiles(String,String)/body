{
  int i1;
  int i2;
  FileInputStream fis1=null;
  FileInputStream fis2=null;
  try {
    fis1=new FileInputStream(f1);
    fis2=new FileInputStream(f2);
  }
 catch (  FileNotFoundException e) {
    return false;
  }
  try {
    try {
      i1=fis1.read();
      i2=fis2.read();
      while ((i1 == i2) && (i1 != -1)) {
        i1=fis1.read();
        i2=fis2.read();
      }
    }
  finally {
      fis1.close();
      fis2.close();
    }
  }
 catch (  IOException e) {
    return false;
  }
  return i1 == i2;
}
