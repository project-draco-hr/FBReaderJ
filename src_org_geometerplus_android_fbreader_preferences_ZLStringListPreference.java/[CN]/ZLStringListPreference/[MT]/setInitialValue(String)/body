{
  if (value == null) {
    return false;
  }
  int index=-1;
  final CharSequence[] entryValues=getEntryValues();
  for (int i=0; i < entryValues.length; ++i) {
    if (value.equals(entryValues[i])) {
      index=i;
      break;
    }
  }
  if (index >= 0) {
    setValueIndex(index);
    setSummary(getEntry());
    return true;
  }
  return false;
}
