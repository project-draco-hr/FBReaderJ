{
  char[] block=myCurrentDataBlock;
  if ((block == null) || (minimumLength > block.length - myBlockOffset)) {
    if (block != null) {
      myStorage.freezeLastBlock();
    }
    block=myStorage.createNewBlock(minimumLength);
    myCurrentDataBlock=block;
    myBlockOffset=0;
  }
  return block;
}
