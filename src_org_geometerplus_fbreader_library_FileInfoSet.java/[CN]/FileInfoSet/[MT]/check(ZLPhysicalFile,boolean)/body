{
  if (file == null) {
    return true;
  }
  final long fileSize=file.size();
  FileInfo info=get(file);
  if (info.FileSize == fileSize) {
    return true;
  }
 else {
    info.FileSize=fileSize;
    if (processChildren) {
      removeChildren(info);
      myInfosToSave.add(info);
      addChildren(file);
    }
 else {
      myInfosToSave.add(info);
    }
    return false;
  }
}
