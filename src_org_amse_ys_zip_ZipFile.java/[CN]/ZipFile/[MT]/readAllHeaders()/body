{
  if (myAllFilesAreRead) {
    return;
  }
  myAllFilesAreRead=true;
  MyBufferedInputStream baseStream=getBaseStream();
  baseStream.setPosition(0);
  myFileHeaders.clear();
  try {
    while (true) {
      readFileHeader(baseStream,null);
    }
  }
  finally {
    storeBaseStream(baseStream);
  }
}
