{
  myDatabase.execSQL("CREATE TABLE Bookmarks(" + "bookmark_id INTEGER PRIMARY KEY," + "book_id INTEGER NOT NULL REFERENCES Books(book_id),"+ "bookmark_text TEXT NOT NULL,"+ "creation_time INTEGER NOT NULL,"+ "modification_time INTEGER,"+ "access_time INTEGER,"+ "access_counter INTEGER NOT NULL,"+ "paragraph INTEGER NOT NULL,"+ "word INTEGER NOT NULL,"+ "char INTEGER NOT NULL)");
}
