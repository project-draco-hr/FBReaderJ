{
  LinkedList<Bookmark> list=new LinkedList<Bookmark>();
  Cursor cursor=myDatabase.rawQuery("SELECT bookmark_id,book_id,bookmark_text,creation_time,modification_time,access_time,access_counter,paragraph,word,char FROM Bookmarks",null);
  while (cursor.moveToNext()) {
    list.add(createBookmark(cursor.getLong(0),cursor.getLong(1),cursor.getString(2),getDate(cursor,3),getDate(cursor,4),getDate(cursor,5),(int)cursor.getLong(6),(int)cursor.getLong(7),(int)cursor.getLong(8),(int)cursor.getLong(9)));
  }
  cursor.close();
  return list;
}
