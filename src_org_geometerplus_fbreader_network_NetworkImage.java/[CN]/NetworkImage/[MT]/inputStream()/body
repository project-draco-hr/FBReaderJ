{
  if (myFileImage == null) {
    if (!isSynchronized()) {
      return null;
    }
    final String path=getFilePath();
    if (path == null) {
      return null;
    }
    final ZLFile file=ZLFile.createFileByPath(path);
    if (file == null) {
      return null;
    }
    myFileImage=new ZLFileImage(myMimeType,file);
  }
  return myFileImage.inputStream();
}
