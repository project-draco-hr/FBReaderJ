{
  if (!myIsSynchronized) {
    String value;
    if (myDefaultValue) {
      value=myConfig.getValue(myGroup,myOptionName,"true");
    }
 else {
      value=myConfig.getValue(myGroup,myOptionName,"false");
    }
    myValue=ZLFromStringConverter.getBooleanValue(value);
    myIsSynchronized=true;
  }
  return myValue;
}
