{
  processByteData();
  tag=tag.toLowerCase().intern();
  if (tag == "br") {
    if (myBuffer.length() > 0) {
      myBuffer.append('\n');
    }
  }
 else   if (tag == "hr") {
    if (myBuffer.length() > 0) {
      if (myBuffer.charAt(myBuffer.length() - 1) != '\n') {
        myBuffer.append('\n');
      }
      myBuffer.append('\n');
    }
  }
}
