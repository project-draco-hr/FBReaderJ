{
  if (!myIsSynchronized) {
    String value=getConfigValue(null);
    if (value != null) {
      try {
        int intValue=Integer.parseInt(value);
        if (myValue.getIntValue() != intValue) {
          myValue=new ZLColor(intValue);
        }
      }
 catch (      NumberFormatException e) {
      }
    }
    myIsSynchronized=true;
  }
  return myValue;
}
