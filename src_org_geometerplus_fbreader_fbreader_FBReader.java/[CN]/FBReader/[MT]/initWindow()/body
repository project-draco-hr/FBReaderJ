{
  super.initWindow();
  refreshWindow();
  String fileName=null;
  if (myArg0 != null) {
    try {
      fileName=new File(myArg0).getCanonicalPath();
    }
 catch (    IOException e) {
    }
  }
  if (!openFile(fileName) && !openFile(myBookNameOption.getValue())) {
    openFile(BookCollection.Instance().getHelpFile());
  }
}
