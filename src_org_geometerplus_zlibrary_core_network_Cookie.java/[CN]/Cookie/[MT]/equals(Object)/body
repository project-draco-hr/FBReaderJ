{
  if (this == o) {
    return true;
  }
  if (!(o instanceof Cookie)) {
    return false;
  }
  final Cookie c=(Cookie)o;
  return Host.equals(c.Host) && Path.equals(c.Path) && Name.equals(c.Name)&& ((Ports == null && c.Ports == null) || Ports.equals(c.Ports))&& Secure == c.Secure && Discard == c.Discard;
}
