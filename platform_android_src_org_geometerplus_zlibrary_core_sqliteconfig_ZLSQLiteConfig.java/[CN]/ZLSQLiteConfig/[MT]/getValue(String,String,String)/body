{
  Cursor cursor=myOpenHelper.getReadableDatabase().query(true,ourTableName,ourColumns,"groupName = ? AND name = ?",new String[]{group,name},null,null,null,null);
  String answer=defaultValue;
  if (cursor.getCount() != 0) {
    cursor.moveToFirst();
    answer=cursor.getString(0);
  }
  cursor.close();
  return answer;
}
