{
  if ((index < 0) || (index >= mySize)) {
    return null;
  }
  if (index == 0) {
    return (T)this;
  }
  --index;
  for (  T subtree : mySubTrees) {
    if (subtree.mySize <= index) {
      index-=subtree.mySize;
    }
 else {
      return (T)subtree.getTree(index);
    }
  }
  throw new RuntimeException("That's impossible!!!");
}
