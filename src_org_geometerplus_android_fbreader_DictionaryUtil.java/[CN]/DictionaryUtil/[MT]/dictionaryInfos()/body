{
  if (ourDictionaryInfos.isEmpty()) {
    ourDictionaryInfos.add(new PackageInfo("com.socialnmobile.colordict","com.socialnmobile.colordict.activity.Main","ColorDict"));
    ourDictionaryInfos.add(new PackageInfo("com.ngc.fora","com.ngc.fora.ForaDictionary","Fora Dictionary"));
    ourDictionaryInfos.add(new PackageInfo("org.freedictionary","org.freedictionary.MainActivity","Free Dictionary . org"));
  }
  return ourDictionaryInfos;
}
