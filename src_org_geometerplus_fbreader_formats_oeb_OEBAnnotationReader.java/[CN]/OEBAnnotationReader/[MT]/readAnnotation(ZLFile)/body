{
  myReadState=READ_NONE;
  myBuffer.delete(0,myBuffer.length());
  if (ZLXMLProcessor.read(this,file,512)) {
    final int len=myBuffer.length();
    if (len > 1) {
      if (myBuffer.charAt(len - 1) == '\n') {
        myBuffer.delete(len - 1,len);
      }
      return myBuffer.toString();
    }
  }
  return null;
}
