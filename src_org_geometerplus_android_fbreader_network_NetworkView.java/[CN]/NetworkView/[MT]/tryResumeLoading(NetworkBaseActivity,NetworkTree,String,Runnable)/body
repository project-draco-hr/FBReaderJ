{
  final ItemsLoadingRunnable runnable=NetworkView.Instance().getItemsLoadingRunnable(key);
  if (runnable != null && runnable.tryResume()) {
    openTree(activity,tree,key);
    return;
  }
  if (runnable == null) {
    expandRunnable.run();
  }
 else {
    runnable.runOnFinish(expandRunnable);
  }
}
