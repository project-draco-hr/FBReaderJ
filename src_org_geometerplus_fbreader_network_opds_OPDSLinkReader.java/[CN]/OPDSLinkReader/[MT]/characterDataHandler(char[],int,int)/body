{
switch (myState) {
case READ_NOTHING:
case READ_SEARCH_DESCRIPTION:
case READ_FEEDS:
case READ_URL_REWRITING_RULES:
case READ_RELATION_ALIASES:
    break;
case READ_SITENAME:
case READ_TITLE:
case READ_SUMMARY:
case READ_ICON_NAME:
case READ_LINK:
case READ_SEARCH_FIELD:
case READ_FEEDS_CONDITION:
  myBuffer.append(data,start,length);
break;
}
}
