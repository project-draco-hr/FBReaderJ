{
  String input="test/org/test/zlibrary/options/example/";
  ZLConfigReaderFactory.createConfigReader(input).read();
  ZLStringOption myOption=new ZLStringOption("ui","MYGROUP","length","12312");
  myOption.setValue("I'M_CHANGED!");
  ZLStringOption myChangeableOption=new ZLStringOption("ui","MYGROUP","lengthChangeable","12312");
  myChangeableOption.setValue("I_COULD_CHANGE!");
  ZLConfigInstance.getInstance().unsetValue("MYGROUP","lengthChangeable");
  String output="test/org/test/zlibrary/options/example/output/output/";
  ZLConfigWriterFactory.createConfigWriter(output).write();
}
