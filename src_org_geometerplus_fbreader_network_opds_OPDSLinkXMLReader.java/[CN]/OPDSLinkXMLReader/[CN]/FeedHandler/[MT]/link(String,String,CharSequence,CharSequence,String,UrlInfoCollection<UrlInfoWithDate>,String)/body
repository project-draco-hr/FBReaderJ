{
  final String titleString=title.toString();
  final String summaryString=summary != null ? summary.toString() : null;
  OPDSNetworkLink opdsLink=new OPDSPredefinedNetworkLink(OPDSNetworkLink.INVALID_ID,id,siteName,titleString,summaryString,language,infos);
  opdsLink.setRelationAliases(myRelationAliases);
  opdsLink.setUrlRewritingRules(myUrlRewritingRules);
  opdsLink.setExtraData(myExtraData);
  if (myAuthenticationType == "litres") {
    opdsLink.setAuthenticationManager(NetworkAuthenticationManager.createManager(opdsLink,sslCertificate,LitResAuthenticationManager.class));
  }
  return opdsLink;
}
