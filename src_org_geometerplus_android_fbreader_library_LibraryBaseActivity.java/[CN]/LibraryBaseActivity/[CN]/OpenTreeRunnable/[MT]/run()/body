{
  if (LibraryInstance.hasState(Library.STATE_FULLY_INITIALIZED)) {
    myPostRunnable.run();
  }
 else {
    UIUtil.runWithMessage(LibraryBaseActivity.this,"loadingBookList",new Runnable(){
      public void run(){
        LibraryInstance.waitForState(Library.STATE_FULLY_INITIALIZED);
      }
    }
,myPostRunnable);
  }
}
