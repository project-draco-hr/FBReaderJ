{
  event=event & ALL_EVENTS;
  System.err.println("Event " + event + " on "+ path);
switch (event) {
case MOVE_SELF:
    break;
case MOVED_TO:
  break;
case MOVED_FROM:
case DELETE:
break;
case DELETE_SELF:
break;
case CLOSE_WRITE:
case ATTRIB:
break;
default :
System.err.println("Unexpected event " + event + " on "+ path);
break;
}
}
