{
switch (type) {
case Creation:
    return myCreationDate;
case Modification:
  return myModificationDate;
case Access:
return myAccessDate;
default :
case Latest:
{
Date latest=myModificationDate;
if (latest == null) {
  latest=myCreationDate;
}
if (myAccessDate != null && latest.compareTo(myAccessDate) < 0) {
  return myAccessDate;
}
 else {
  return latest;
}
}
}
}
