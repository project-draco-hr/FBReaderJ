{
  if (mySearchResultsAdapter == null) {
    mySearchResultsAdapter=new LibraryAdapter(createTab("searchResults",R.id.search_results,R.drawable.ic_tab_library_results),tree,Type.FLAT);
  }
 else {
    mySearchResultsAdapter.resetTree(tree);
  }
  getTabHost().setCurrentTabByTag("searchResults");
}
