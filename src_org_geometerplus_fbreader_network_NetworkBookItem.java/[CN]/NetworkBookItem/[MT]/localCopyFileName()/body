{
  final boolean hasBuyReference=this.reference(UrlInfo.Type.BookBuy) != null || this.reference(UrlInfo.Type.BookBuyInBrowser) != null;
  BookUrlInfo reference=null;
  String fileName=null;
  for (  UrlInfo r : getAllInfos()) {
    if (!(r instanceof BookUrlInfo)) {
      continue;
    }
    final BookUrlInfo br=(BookUrlInfo)r;
    final UrlInfo.Type type=br.InfoType;
    if ((type == UrlInfo.Type.Book || type == UrlInfo.Type.BookConditional || (!hasBuyReference && type == UrlInfo.Type.BookFullOrDemo)) && (reference == null || br.BookFormat > reference.BookFormat)) {
      String name=br.localCopyFileName(UrlInfo.Type.Book);
      if (name != null) {
        reference=br;
        fileName=name;
      }
    }
  }
  return fileName;
}
