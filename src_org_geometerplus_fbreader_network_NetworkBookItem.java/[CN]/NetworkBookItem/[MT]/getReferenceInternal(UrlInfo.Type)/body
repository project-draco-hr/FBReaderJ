{
  BookUrlInfo reference=null;
  for (  UrlInfo r : getAllInfos(type)) {
    if (!(r instanceof BookUrlInfo)) {
      continue;
    }
    final BookUrlInfo br=(BookUrlInfo)r;
    if (reference == null || br.BookFormat.compareTo(reference.BookFormat) > 0) {
      reference=br;
    }
  }
  return reference;
}
