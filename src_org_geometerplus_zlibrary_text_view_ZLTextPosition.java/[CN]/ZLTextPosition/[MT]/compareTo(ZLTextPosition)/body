{
  final int p0=getParagraphIndex();
  final int p1=position.getParagraphIndex();
  if (p0 != p1) {
    return p0 < p1 ? -1 : 1;
  }
  final int e0=getElementIndex();
  final int e1=position.getElementIndex();
  if (e0 != e1) {
    return e0 < e1 ? -1 : 1;
  }
  final int c0=getCharIndex();
  final int c1=position.getCharIndex();
  if (c0 != c1) {
    return c0 < c1 ? -1 : 1;
  }
  return 0;
}
