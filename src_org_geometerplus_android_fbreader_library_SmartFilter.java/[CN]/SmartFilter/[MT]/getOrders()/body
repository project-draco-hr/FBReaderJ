{
  if (myFile == null)   myParent.runOnUiThread(myReturnRes);
  for (  ZLFile file : myFile.children()) {
    if (!Thread.currentThread().isInterrupted()) {
      String path=file.getPath();
      String name=file.getName(false).substring(file.getName(false).lastIndexOf('/') + 1);
      if (file.isDirectory())       myOrders.add(new FileOrder(name,path,R.drawable.ic_list_library_folder));
 else       if (file.isArchive())       myOrders.add(new FileOrder(name,path,R.drawable.fbreader));
 else       if (PluginCollection.Instance().getPlugin(file) != null)       myOrders.add(new FileOrder(name,path,R.drawable.ic_list_library_book));
      myParent.runOnUiThread(myReturnRes);
    }
  }
  myParent.runOnUiThread(myReturnRes);
}
