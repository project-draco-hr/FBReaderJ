{
  try {
    for (    ZLFile file : myFile.children()) {
      if (!Thread.currentThread().isInterrupted()) {
        if (file.isDirectory() || file.isArchive() || PluginCollection.Instance().getPlugin(file) != null) {
          myItems.add(new FileItem(file));
          myParent.runOnUiThread(myReturnRes);
        }
      }
    }
  }
 catch (  Exception e) {
    myParent.runOnUiThread(new Runnable(){
      public void run(){
        Toast.makeText(myParent,ZLResource.resource("fmanagerView").getResource("permission_denied").getValue(),Toast.LENGTH_SHORT).show();
      }
    }
);
    myParent.finish();
  }
}
