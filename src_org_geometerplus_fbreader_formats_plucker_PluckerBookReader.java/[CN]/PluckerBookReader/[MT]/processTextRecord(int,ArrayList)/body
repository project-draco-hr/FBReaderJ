{
  int start=0;
  int end=0;
  for (Iterator it=pars.iterator(); it.hasNext(); ) {
    start=end;
    end=start + (Integer)it.next();
    if (end > size) {
      return;
    }
    myParagraphStored=false;
    processTextParagraph(myCharBuffer,start,end);
    if (!myParagraphStored) {
      myParagraphVector.add(-1);
    }
  }
}
