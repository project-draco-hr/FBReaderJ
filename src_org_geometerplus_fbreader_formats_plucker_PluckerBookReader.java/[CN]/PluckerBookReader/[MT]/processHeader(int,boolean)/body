{
  if (start) {
    enterTitle();
    int kind;
switch (font) {
case FontType.FT_H1:
      kind=FBTextKind.H1;
    break;
case FontType.FT_H2:
  kind=FBTextKind.H2;
break;
case FontType.FT_H3:
kind=FBTextKind.H3;
break;
case FontType.FT_H4:
kind=FBTextKind.H4;
break;
case FontType.FT_H5:
kind=FBTextKind.H5;
break;
case FontType.FT_H6:
default :
kind=FBTextKind.H6;
break;
}
pushKind((byte)kind);
}
 else {
popKind();
exitTitle();
}
}
