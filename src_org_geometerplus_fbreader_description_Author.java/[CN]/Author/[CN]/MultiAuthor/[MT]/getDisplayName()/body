{
  if ((myDisplayName.length() == 0) && (myAuthors.size() != 0)) {
    myDisplayName=((Author)myAuthors.get(0)).getDisplayName();
    for (int i=1; i < myAuthors.size(); ++i) {
      myDisplayName+=", ";
      myDisplayName+=((Author)myAuthors.get(i)).getDisplayName();
    }
  }
  return myDisplayName;
}
