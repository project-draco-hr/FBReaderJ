{
  final Cursor cursor=myDatabase.rawQuery("SELECT BookLabel.book_id FROM BookLabel" + " INNER JOIN Labels ON BookLabel.label_id=Labels.label_id" + " WHERE BookLabel.book_id=? AND Labels.name='favorite' LIMIT 1",new String[]{String.valueOf(bookId)});
  boolean result=cursor.moveToNext();
  cursor.close();
  return result;
}
