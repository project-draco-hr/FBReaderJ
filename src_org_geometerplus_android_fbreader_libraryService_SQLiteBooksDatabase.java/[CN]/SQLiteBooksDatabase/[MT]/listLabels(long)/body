{
  final Cursor cursor=myDatabase.rawQuery("SELECT Labels.name FROM Labels" + " INNER JOIN BookLabel ON BookLabel.label_id=Labels.label_id" + " WHERE BookLabel.book_id=?",new String[]{String.valueOf(bookId)});
  final LinkedList<String> names=new LinkedList<String>();
  while (cursor.moveToNext()) {
    names.add(cursor.getString(0));
  }
  cursor.close();
  return names;
}
