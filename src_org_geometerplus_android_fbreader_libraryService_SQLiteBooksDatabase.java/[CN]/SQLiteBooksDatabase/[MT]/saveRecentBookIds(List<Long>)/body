{
  final SQLiteStatement statement=get("INSERT OR IGNORE INTO RecentBooks (book_id) VALUES (?)");
  executeAsTransaction(new Runnable(){
    public void run(){
      myDatabase.delete("RecentBooks",null,null);
      for (      long id : ids) {
        statement.bindLong(1,id);
        statement.execute();
      }
    }
  }
);
}
