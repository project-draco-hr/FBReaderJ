{
  myDatabase.execSQL("INSERT OR IGNORE INTO Labels (name) VALUES (?)",new Object[]{label});
  final SQLiteStatement statement=get("INSERT OR IGNORE INTO BookLabel(label_id,book_id)" + " SELECT label_id,? FROM Labels WHERE name=?");
  statement.bindLong(1,bookId);
  statement.bindString(2,label);
  statement.execute();
}
