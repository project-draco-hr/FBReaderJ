{
  int left=0;
  int right=size();
  while (left < right) {
    final int middle=(left + right) / 2;
    final ZLTextRectangularArea candidate=(ZLTextRectangularArea)get(middle);
    if (candidate.YStart > y) {
      right=middle;
    }
 else     if (candidate.YEnd < y) {
      left=middle + 1;
    }
 else {
      return candidate;
    }
  }
  return null;
}
