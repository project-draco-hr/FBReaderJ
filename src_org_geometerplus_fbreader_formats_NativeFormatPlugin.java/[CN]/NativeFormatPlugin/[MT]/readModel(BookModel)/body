{
  final int code=readModelNative(model);
switch (code) {
case 0:
    return;
case 3:
  throw new CachedCharStorageException("Cannot write file from native code");
default :
throw new BookReadingException("nativeCodeFailure",model.Book.File,new String[]{String.valueOf(code),model.Book.File.getPath()});
}
}
