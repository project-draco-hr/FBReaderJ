{
  ZLFile file=new ZLFile(path);
  try {
    PdbStream stream=new PluckerTextStream(file);
    if (stream.open()) {
      detectEncodingAndLanguage(description,stream);
      stream.close();
    }
  }
 catch (  IOException e) {
  }
  if (description.getEncoding().length() == 0) {
    return false;
  }
  return true;
}
