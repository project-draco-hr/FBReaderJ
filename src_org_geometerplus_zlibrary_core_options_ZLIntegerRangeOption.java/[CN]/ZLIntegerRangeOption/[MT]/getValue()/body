{
  if (!myIsSynchronized) {
    String value=getConfigValue(null);
    if (value != null) {
      try {
        int intValue=Integer.parseInt(value);
        if (intValue < MinValue) {
          intValue=MinValue;
        }
 else         if (intValue > MaxValue) {
          intValue=MaxValue;
        }
        myValue=intValue;
      }
 catch (      NumberFormatException e) {
      }
    }
    myIsSynchronized=true;
  }
  return myValue;
}
