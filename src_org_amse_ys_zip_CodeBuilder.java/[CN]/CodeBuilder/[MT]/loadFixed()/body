{
  for (int i=0; i <= 143; i++) {
    myCodeArray[i]=48 + i;
    myLengthArray[i]=8;
  }
  for (int i=144; i <= 255; i++) {
    myLengthArray[i]=9;
    myCodeArray[i]=256 + i;
  }
  for (int i=256; i <= 279; i++) {
    myLengthArray[i]=7;
    myCodeArray[i]=0 + i - 256;
  }
  for (int i=280; i <= 287; i++) {
    myCodeArray[i]=192 + i - 280;
    myLengthArray[i]=8;
  }
}
